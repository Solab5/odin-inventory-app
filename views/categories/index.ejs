<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <%- include('../header') %>
    
    <h1><%= title %></h1>
    <ul>
        <% categories.forEach((category) => { %>
            <li>
                <a href="/categories/<%= category.id %>"><%= category.name %></a>
            </li>
        <% }) %>
    </ul>
    <a href="/categories/new">Add New Category</a>
    
    <%- include('../footer') %>
</body>
</html>